<UserControl x:Class="AvaloniaSample.Axes.TimeSpanScaled.View"
             xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             xmlns:vms="using:ViewModelsSamples.Axes.TimeSpanScaled">
  <UserControl.DataContext>
    <vms:ViewModel/>
  </UserControl.DataContext>
  <lvc:CartesianChart
      Series="{Binding Series}"
      XAxes="{Binding XAxes}">
  </lvc:CartesianChart>
</UserControl>
